CREATE TABLESPACE PetLovers_Data
       DATAFILE 'C:\app\Miuyin\oradata\petloversdb\petloversdata01.dbf'
       SIZE 10M
       REUSE
       AUTOEXTEND ON
       NEXT 512k
       MAXSIZE 200M;
       
   
